{
    "name": "Swtich Lang",
    "version": "1.0",
    "author": "Sho Sato",
    "description": "Swtich hl param",
    "content_scripts": [
        {
            "matches": [
                "*://www.google.com/search?*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_start"
        }
    ],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/get_started16.png",
            "32": "images/get_started32.png",
            "48": "images/get_started48.png",
            "128": "images/get_started128.png"
        }
    },
    "web_accessible_resources": [
        "inject.js"
    ],
    "permissions": [
        "webNavigation",
        "tabs",
        "storage",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "scripts": [
            "background.js",
            "hot-reload.js"
        ],
        "persistent": true
    },
    "manifest_version": 2
}